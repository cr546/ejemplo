        <title>Manejo DOM</title>
    </head>
    <Body>
        <h1 onclick="cambiarTexto(this)">"Manejo DOM"</h1>
        
        <script>
            function cambiarTexto(titulo){
                console.log(titulo);
                console.log(titulo.innerHTML);
                titulo.innerHTML="Cambiando el texto";
                console.log(titulo.innerHTML);
                        }
        </script>
    </Body>
